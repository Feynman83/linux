#include <dt-bindings/gpio/gpio.h>
#include <dt-bindings/input/input.h>

/ {
	chosen {
		stdout-path = &uart1;
	};
	regulators {
		compatible = "simple-bus";
		#address-cells = <1>;
		#size-cells = <0>;

		reg_3p3v: regulator@0 {
			compatible = "regulator-fixed";
			reg = <0>;
			regulator-name = "3P3V";
			regulator-min-microvolt = <3300000>;
			regulator-max-microvolt = <3300000>;
			regulator-always-on;
		};

		reg_usbh1_vbus: regulator@1 {
			compatible = "regulator-fixed";
			reg = <1>;
			pinctrl-names = "default";
			regulator-name = "usbh1_vbus";
			regulator-min-microvolt = <5000000>;
			regulator-max-microvolt = <5000000>;
			gpio = <&gpio3 0 GPIO_ACTIVE_HIGH>;
			enable-active-high;
		};

		reg_hub1_rst: regulator@2 {
			compatible = "regulator-fixed";
			reg = <2>;
			pinctrl-names = "default";
			regulator-name = "hub1_rst";
			regulator-min-microvolt = <3300000>;
			regulator-max-microvolt = <3300000>;
			gpio = <&gpio6 4 GPIO_ACTIVE_HIGH>;
			enable-active-high;
			regulator-always-on;
			regulator-boot-on;
		};

		reg_hub2_rst: regulator@3 {
			compatible = "regulator-fixed";
			reg = <3>;
			pinctrl-names = "default";
			regulator-name = "hub2_rst";
			regulator-min-microvolt = <3300000>;
			regulator-max-microvolt = <3300000>;
			gpio = <&gpio6 5 GPIO_ACTIVE_HIGH>;
			enable-active-high;
			regulator-always-on;
			regulator-boot-on;
		};

		
	};

	leds {
		compatible = "gpio-leds";
		pinctrl-names = "default";
		pinctrl-0 = <&pinctrl_led>;

		led0: core {
			label = "core";
			gpios = <&gpio3 18 GPIO_ACTIVE_LOW>;
			default-state = "off";
			linux,default-trigger = "heartbeat";
		};

		led1:run {
			label = "run";
			gpios = <&gpio1 0 GPIO_ACTIVE_LOW>;
			default-state = "off";
			linux,default-trigger = "heartbeat";
		};

		led2:alarm {
			label = "alarm";
			gpios = <&gpio5 4 GPIO_ACTIVE_LOW>;
			default-state = "off";
			linux,default-trigger = "none";
		};
	};

};



&ecspi2 {
	cs-gpios = <&gpio2 26 GPIO_ACTIVE_LOW>;
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_ecspi2>;
	status = "okay";

	adc128s022_0@0 {
		compatible = "ti,adc128s052";
		spi-max-frequency = <250000>;
		reg = <0x0>;
	};
};




&fec {
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_enet>;
	phy-mode = "rgmii-id";
	// phy-handle = <&phy>;
	fsl,magic-packet;
	status = "okay";

	// mdio {
	// 	#address-cells = <1>;
	// 	#size-cells = <0>;

	// 	phy: ethernet-phy@1 {
	// 		reg = <1>;
	// 		qca,clk-out-frequency = <125000000>;
	// 		reset-gpios = <&gpio1 25 GPIO_ACTIVE_LOW>;
	// 		reset-assert-us = <10000>;
	// 	};
	// };
};


&i2c1 {
	clock-frequency = <100000>;
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_i2c1>;
	status = "okay";
	
	eeprom@54 {
		compatible = "atmel,24c04";
		reg = <0x54>;
	};
};

&i2c2 {
	clock-frequency = <100000>;
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_i2c2>;
	status = "okay";

};

&i2c3 {
	clock-frequency = <100000>;
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_i2c3>;
	status = "okay";
};

&iomuxc {
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_hog>;

	imx6qdl-rex {
		pinctrl_hog: hoggrp {
			fsl,pins = <
				
				
				MX6QDL_PAD_CSI0_DAT18__GPIO6_IO04	0x80000000
				MX6QDL_PAD_CSI0_DAT19__GPIO6_IO05	0x80000000
				MX6QDL_PAD_DISP0_DAT7__GPIO4_IO28	0x80000000 //recovery key
				
				MX6QDL_PAD_GPIO_0__GPIO1_IO00		0x80000000 //led-run
				MX6QDL_PAD_EIM_A24__GPIO5_IO04		0x80000000 //led-alarm

				MX6QDL_PAD_DISP0_DAT19__GPIO5_IO13	0x80000000	//DO01
				MX6QDL_PAD_DISP0_DAT20__GPIO5_IO14	0x80000000	//DO02
				MX6QDL_PAD_DISP0_DAT21__GPIO5_IO15	0x80000000	//DO03
				MX6QDL_PAD_DISP0_DAT22__GPIO5_IO16	0x80000000	//DO04
				MX6QDL_PAD_SD3_DAT0__GPIO7_IO04		0x80000000	//DO01-back
				MX6QDL_PAD_SD3_DAT1__GPIO7_IO05		0x80000000	//DO02-back
				MX6QDL_PAD_SD3_DAT2__GPIO7_IO06		0x80000000	//DO03-back
				MX6QDL_PAD_SD3_DAT3__GPIO7_IO07		0x80000000	//DO04-back
				MX6QDL_PAD_DISP0_DAT16__GPIO5_IO10	0x80000000	//DO-OE
				MX6QDL_PAD_DISP0_DAT23__GPIO5_IO17	0x80000000	//DO-LE

				MX6QDL_PAD_DISP0_DAT0__GPIO4_IO21	0x80000000	//DI01
				MX6QDL_PAD_DISP0_DAT1__GPIO4_IO22	0x80000000	//DI02
				MX6QDL_PAD_DISP0_DAT2__GPIO4_IO23	0x80000000	//DI03
				MX6QDL_PAD_DISP0_DAT3__GPIO4_IO24	0x80000000	//DI04
				MX6QDL_PAD_DISP0_DAT4__GPIO4_IO25	0x80000000	//DI05
				MX6QDL_PAD_DISP0_DAT5__GPIO4_IO26	0x80000000	//DI06
				MX6QDL_PAD_DISP0_DAT10__GPIO4_IO31	0x80000000	//DI07
				MX6QDL_PAD_DISP0_DAT11__GPIO5_IO05	0x80000000	//DI08
				MX6QDL_PAD_DISP0_DAT12__GPIO5_IO06	0x80000000	//DI09
				MX6QDL_PAD_DISP0_DAT13__GPIO5_IO07	0x80000000	//DI10
				MX6QDL_PAD_DISP0_DAT14__GPIO5_IO08	0x80000000	//DI11
				MX6QDL_PAD_DISP0_DAT15__GPIO5_IO09	0x80000000	//DI12

				MX6QDL_PAD_DI0_PIN15__GPIO4_IO17	0x80000000	//AI01-switch
				MX6QDL_PAD_DI0_PIN3__GPIO4_IO19		0x80000000	//AI02-switch
				MX6QDL_PAD_DI0_PIN2__GPIO4_IO18		0x80000000	//AI03-switch
				MX6QDL_PAD_DI0_DISP_CLK__GPIO4_IO16	0x80000000	//AI04-switch

				MX6QDL_PAD_KEY_ROW2__GPIO4_IO11		0x80000000	//uart1-485/232switch
				MX6QDL_PAD_KEY_ROW3__GPIO4_IO13		0x80000000	//uart1-485/232switch

		
				MX6QDL_PAD_DISP0_DAT18__GPIO5_IO12	0x80000000	//4g-pwr
				MX6QDL_PAD_DISP0_DAT6__GPIO4_IO27	0x80000000	//4g-open
			>;
		};

		

		pinctrl_ecspi1: ecspi1grp {
			fsl,pins = <
				MX6QDL_PAD_DISP0_DAT20__ECSPI1_SCLK 0x100b1 
				MX6QDL_PAD_DISP0_DAT21__ECSPI1_MOSI 0x100b1 
				MX6QDL_PAD_DISP0_DAT22__ECSPI1_MISO 0x100b1
				/* CS */
				MX6QDL_PAD_DISP0_DAT23__GPIO5_IO17 0x80000000
			>;
		};
		
		pinctrl_ecspi2: ecspi2grp {
			fsl,pins = <
				MX6QDL_PAD_EIM_CS0__ECSPI2_SCLK	0x100b1 
				MX6QDL_PAD_EIM_CS1__ECSPI2_MOSI	0x100b1 
				MX6QDL_PAD_EIM_OE__ECSPI2_MISO	0x100b1
				/* CS */
				MX6QDL_PAD_EIM_RW__GPIO2_IO26	0x000b1
			>;
		};

		pinctrl_ecspi3: ecspi3grp {
			fsl,pins = <
				MX6QDL_PAD_DISP0_DAT2__ECSPI3_MISO 0x100b1 
                MX6QDL_PAD_DISP0_DAT1__ECSPI3_MOSI 0x100b1 
                MX6QDL_PAD_DISP0_DAT0__ECSPI3_SCLK 0x100b1
				/* CS */
				MX6QDL_PAD_DISP0_DAT3__GPIO4_IO24 0x80000000
			>;
		};

		pinctrl_ecspi4: ecspi4grp {
			fsl,pins = <
				MX6QDL_PAD_EIM_D21__ECSPI4_SCLK 0x100b1 
                MX6QDL_PAD_EIM_D22__ECSPI4_MISO 0x100b1 
                MX6QDL_PAD_EIM_D28__ECSPI4_MOSI 0x100b1
				/* CS */
				MX6QDL_PAD_EIM_D20__GPIO3_IO20 0x80000000
			>;
		};

		pinctrl_enet: enetgrp {
			fsl,pins = <
				MX6QDL_PAD_ENET_MDIO__ENET_MDIO		0x1b0b0
				MX6QDL_PAD_ENET_MDC__ENET_MDC		0x1b0b0
				MX6QDL_PAD_RGMII_TXC__RGMII_TXC		0x1b030
				MX6QDL_PAD_RGMII_TD0__RGMII_TD0		0x1b030
				MX6QDL_PAD_RGMII_TD1__RGMII_TD1		0x1b030
				MX6QDL_PAD_RGMII_TD2__RGMII_TD2		0x1b030
				MX6QDL_PAD_RGMII_TD3__RGMII_TD3		0x1b030
				MX6QDL_PAD_RGMII_TX_CTL__RGMII_TX_CTL	0x1b030
				MX6QDL_PAD_ENET_REF_CLK__ENET_TX_CLK	0x1b0b0
				MX6QDL_PAD_RGMII_RXC__RGMII_RXC		0x1b030
				MX6QDL_PAD_RGMII_RD0__RGMII_RD0		0x1b030
				MX6QDL_PAD_RGMII_RD1__RGMII_RD1		0x1b030
				MX6QDL_PAD_RGMII_RD2__RGMII_RD2		0x1b030
				MX6QDL_PAD_RGMII_RD3__RGMII_RD3		0x1b030
				MX6QDL_PAD_RGMII_RX_CTL__RGMII_RX_CTL	0x1b030
				
			>;
		};

		pinctrl_i2c1: i2c1grp {
			fsl,pins = <
				MX6QDL_PAD_CSI0_DAT8__I2C1_SDA		0x4001b8b1
				MX6QDL_PAD_CSI0_DAT9__I2C1_SCL		0x4001b8b1
			>;
		};

		pinctrl_i2c2: i2c2grp {
			fsl,pins = <
				MX6QDL_PAD_EIM_D16__I2C2_SDA 0x4001b8b1 
				MX6QDL_PAD_EIM_EB2__I2C2_SCL 0x4001b8b1
			>;
		};

		pinctrl_i2c3: i2c3grp {
			fsl,pins = <
				MX6QDL_PAD_GPIO_3__I2C3_SCL 0x4001b8b1 
				MX6QDL_PAD_GPIO_6__I2C3_SDA 0x4001b8b1
			>;
		};

		pinctrl_led: ledgrp {
			fsl,pins = <
				/* run led */
				MX6QDL_PAD_GPIO_0__GPIO1_IO00 0x80000000
				/* alarm led */
				MX6QDL_PAD_EIM_A24__GPIO5_IO04 0x80000000 
				/* core led */
				MX6QDL_PAD_EIM_D18__GPIO3_IO18 0x80000000
			>;
		};

		pinctrl_tca6424: tca6424grp {
			fsl,pins = <
				MX6QDL_PAD_DISP0_DAT16__GPIO5_IO10	0x17059
		   >;
		};

		pinctrl_uart1: uart1grp {
			fsl,pins = <
				MX6QDL_PAD_CSI0_DAT10__UART1_TX_DATA	0x1b0b1
				MX6QDL_PAD_CSI0_DAT11__UART1_RX_DATA	0x1b0b1
			>;
		};

		pinctrl_uart2: uart2grp {
			fsl,pins = <
				MX6QDL_PAD_GPIO_7__UART2_TX_DATA 	0x1b0b1 
				MX6QDL_PAD_GPIO_8__UART2_RX_DATA 	0x1b0b1 
			>;
		};
		pinctrl_uart2_rts: uart2grp_rts {
			fsl,pins = <
				MX6QDL_PAD_KEY_ROW4__GPIO4_IO15 	0x4001b0b1
			>;
		};

		pinctrl_uart3: uart3grp {
			fsl,pins = <
				MX6QDL_PAD_EIM_D24__UART3_TX_DATA 	0x1b0b1 
				MX6QDL_PAD_EIM_D25__UART3_RX_DATA 	0x1b0b1 
				
			>;
		};
		pinctrl_uart3_rts: uart3grp_rts {
			fsl,pins = <
				MX6QDL_PAD_KEY_COL2__GPIO4_IO10 	0x4001b0b1
			>;
		};
		
		pinctrl_uart4: uart4grp {
			fsl,pins = <
				MX6QDL_PAD_KEY_COL0__UART4_TX_DATA 	0x1b0b1 
				MX6QDL_PAD_KEY_ROW0__UART4_RX_DATA 	0x1b0b1 
			>;
		};
		pinctrl_uart4_rts: uart4grp_rts {
			fsl,pins = <
				MX6QDL_PAD_KEY_COL3__GPIO4_IO12 	0x4001b0b1
			>;
		};

		pinctrl_uart5: uart5grp {
			fsl,pins = <
				MX6QDL_PAD_KEY_COL1__UART5_TX_DATA 	0x1b0b1 
				MX6QDL_PAD_KEY_ROW1__UART5_RX_DATA 	0x1b0b1 
			>;
		};
		pinctrl_uart5_rts: uart5grp_rts {
			fsl,pins = <
				MX6QDL_PAD_KEY_COL4__GPIO4_IO14 	0x4001b0b1
			>;
		};


		pinctrl_usbh1: usbh1grp {
			fsl,pins = <
				/* power enable, high active */
				MX6QDL_PAD_EIM_DA0__GPIO3_IO00		0x10b0
			>;
		};

		pinctrl_usbotg: usbotggrp {
			fsl,pins = <
				MX6QDL_PAD_ENET_RX_ER__USB_OTG_ID	0x17059
			>;
		};

		pinctrl_usdhc2: usdhc2grp {
			fsl,pins = <
				MX6QDL_PAD_SD2_CMD__SD2_CMD		0x17059
				MX6QDL_PAD_SD2_CLK__SD2_CLK		0x10059
				MX6QDL_PAD_SD2_DAT0__SD2_DATA0		0x17059
				MX6QDL_PAD_SD2_DAT1__SD2_DATA1		0x17059
				MX6QDL_PAD_SD2_DAT2__SD2_DATA2		0x17059
				MX6QDL_PAD_SD2_DAT3__SD2_DATA3		0x17059
				/* CD */
				MX6QDL_PAD_GPIO_4__GPIO1_IO04		0x1b0b0
			>;
		};

		pinctrl_usdhc4: usdhc4grp {
			fsl,pins = <
				MX6QDL_PAD_SD4_CMD__SD4_CMD 0x17059 
				MX6QDL_PAD_SD4_CLK__SD4_CLK 0x10059 
				MX6QDL_PAD_SD4_DAT0__SD4_DATA0 0x17059 
				MX6QDL_PAD_SD4_DAT1__SD4_DATA1 0x17059 
				MX6QDL_PAD_SD4_DAT2__SD4_DATA2 0x17059 
				MX6QDL_PAD_SD4_DAT3__SD4_DATA3 0x17059 
				MX6QDL_PAD_SD4_DAT4__SD4_DATA4 0x17059 
				MX6QDL_PAD_SD4_DAT5__SD4_DATA5 0x17059 
				MX6QDL_PAD_SD4_DAT6__SD4_DATA6 0x17059 
				MX6QDL_PAD_SD4_DAT7__SD4_DATA7 0x17059
			>;
		};
	};
};


&uart1 {
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_uart1>;
	status = "okay";
};

&uart2 {
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_uart2>;
	status = "okay";
	
};

&uart3 {
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_uart3>;
	status = "okay";
};

&uart4 {
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_uart4>;
	status = "okay";
};

&uart5 {
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_uart5>;
	status = "okay";
};

&usbh1 {
	vbus-supply = <&reg_usbh1_vbus>;
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_usbh1>;
	status = "okay";
};

&usbotg {
	// vbus-supply = <&reg_usb_otg_vbus>;
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_usbotg>;
	status = "okay";
};

&usdhc2 {
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_usdhc2>;
	bus-width = <4>;
	cd-gpios = <&gpio1 4 GPIO_ACTIVE_LOW>;
	status = "okay";
};

&usdhc4 {
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_usdhc4>;
	bus-width = <8>;
	status = "okay";
	non-removable;
	// no-1-8-v;
	keep-power-in-suspend;
};


